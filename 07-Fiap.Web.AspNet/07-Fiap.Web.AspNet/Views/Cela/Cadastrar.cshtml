@model Cela
@{
    ViewData["Title"] = "Cadastrar";
}
<h1>Cadastrar</h1>
@TempData["msg"]
<form asp-action="Cadastrar">
    <div class="form-group">
        <label asp-for="Nome"></label>
        <input asp-for="Nome" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Area"></label>
        <input asp-for="Area" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="QuantidadeMaxima"></label>
        <input asp-for="QuantidadeMaxima" class="form-control">
    </div>       
    <botao texto="Cadastar"></botao>
</form>