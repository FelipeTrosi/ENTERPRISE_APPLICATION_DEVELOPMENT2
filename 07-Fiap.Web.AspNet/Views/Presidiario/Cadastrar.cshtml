@model Presidiario
@{
    ViewData["Title"] = "Cadastro";
}

<h1>Cadastro</h1>

<form asp-action="Cadastrar">
    <div class="form-group">
        <label asp-for="Nome"></label>
        <input asp-for="Nome" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DataEntrada"></label>
        <input asp-for="DataEntrada" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DataSaida"></label>
        <input asp-for="DataSaida" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="NivelEscolaridade"></label>
        <select asp-for="NivelEscolaridade"
                asp-items="@Html.GetEnumSelectList<NivelEscolaridade>()">
            <option>Selecione</option>
        </select>
    </div>
    <div class="form-group">
        <label asp-for="Cela"></label>
        <select asp-for="CelaId" asp-items="@ViewBag.celas">
            <option>Selecione</option>
        </select>
    </div>
    <botao texto="Cadastrar"></botao>
</form>